  "scripts": {
    "postinstall": "npm run build",
    "start": "node index.js",
    "_start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test_mongodb": "nodemon --exec \"mocha --recursive ./test/mongodb\"",
    "test_api": "nodemon --exec \"mocha --recursive ./test/api_tests\"",
    "test:watch": "npm run test -- --watch",
    "start_server_test": "set NODE_ENV=test&& nodemon index.js",
    "gulp": "gulp watch",
    "build": "webpack -p",
    "nodemon:dev": "SET NODE_ENV=dev && nodemon index.js"
  },
