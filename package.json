  },
  "scripts": {
    "start": "node index.js",
    "_start": "SET NODE_ENV=dev && node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test_mongodb": "nodemon --exec \"mocha --recursive ./test/mongodb\"",
    "test_api": "nodemon --exec \"mocha --recursive ./test/api_tests\"",
    "test:watch": "npm run test -- --watch",
    "start_server_test": "set NODE_ENV=test&& nodemon index.js",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "nodemon:dev": "SET NODE_ENV=development && nodemon index.js"
  },
